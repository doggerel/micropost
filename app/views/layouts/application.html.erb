<!DOCTYPE html>
<html>
<head>
  <title><%=title %></title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <!--stylesheet layouts-->
</head>
  <body>
  <div class="container" >
    <div class="span-23 last header">
      <nav class="header-menu global-menu">
      <%=render "layouts/menu"%>
      </nav>
    </div>
    <div class="span-4 first filler-side">
      cat
    </div>
    <div class="span-11 center-panel  lighten left global-menu">
      <%= yield %>
      <%=debug(params) if Rails.env.development?%>
      <%=render "layouts/footer"%>
    </div>
    <div class="span-8 last menu-side left">
      <div class="menu-side">
    <%if @user%> 
      <%=link_to @user.name, users_path(@user)%>
      <%=avatar_url(@user,20)%>
    <%end%>
  </div>
    </div>
  </div>
  </body>
</html>

