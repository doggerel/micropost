<!DOCTYPE html>
<html>
<head>
  <title><%=title %></title>
  <%= stylesheet_link_tag    "application" %>
  <%= stylesheet_link_tag    "jquery.qtip" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "password" %>
  <%= javascript_include_tag 'jquery.qtip', 'formtastic.qtip2' %>

  <%= csrf_meta_tags %>
  <!--stylesheet layouts-->
</head>
  <body>
  <div class="container" >
    <div class="span-23 last header">
      <nav class="header-menu global-menu">
      <%=render "layouts/header"%>
      </nav>
      <%flash.each do |key,value|  %>
        <%=content_tag(:div, value, :class=>"flash #{key}")%>
      <%end %>
    </div>
    <div class="span-4 first filler-side">
      cat
    </div>
    <div class="span-11 center-panel  lighten left global-menu">
      <div class="inner-center-panel">
      <%= yield %>
      <%=debug(params) if Rails.env.development?%>
      <%=render "layouts/footer"%>
    </div>
    </div>
    <div class="span-8 last  left">
      <div class="menu-side">
      </div>
    </div>
  </div>
  </body>
</html>

